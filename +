"""
Main module of the Curiosity Driven Learning Project
"""
import retro


if __name__=="__main__":
    env = retro.make(game="SuperMarioBros-Nes")
    obs = env.reset()
    while True:
        obs, rew, done, info = env.step(env.action_space.sample())
        env.render()
        if done:
            obs = env.reset()
            break
    env.close()
